{
    "name": "genealabs/laravel-changelog",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "illuminate/filesystem": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0",
        "jenssegers/model": "^1.5",
        "spatie/laravel-markdown": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "GeneaLabs\\LaravelChangelog\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GeneaLabs\\LaravelChangelog\\Providers\\Tool"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
